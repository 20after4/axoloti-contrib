   <patchobj type="patch/object" uuid="de267983-1870-4ce1-8855-72b4e2d42912" name="object_1" x="336" y="0">
      <params>
         <int32 name="num" value="1"/>
         <int32 name="den" value="32"/>
         <int32 name="mulstart" value="0"/>
      </params>
      <attribs>
         <objref attributeName="table" obj="alloc_1"/>
      </attribs>
      <object id="patch/object" uuid="de267983-1870-4ce1-8855-72b4e2d42912">
         <inlets>
            <bool32 name="play"/>
            <bool32 name="hold"/>
            <int32 name="length"/>
            <int32 name="startpos"/>
         </inlets>
         <outlets>
            <frac32buffer.positive name="pos"/>
         </outlets>
         <displays/>
         <params>
            <int32 name="num">
               <MinValue i="1"/>
               <MaxValue i="32"/>
            </int32>
            <int32 name="den">
               <MinValue i="1"/>
               <MaxValue i="32"/>
            </int32>
            <int32 name="mulstart">
               <MinValue i="0"/>
               <MaxValue i="31"/>
            </int32>
         </params>
         <attribs>
            <objref name="table"/>
         </attribs>
         <includes/>
         <code.declaration><![CDATA[uint32_t counter = 0;
uint32_t maxlength = attr_table.LENGTH;

bool play;
bool hold;]]></code.declaration>
         <code.krate><![CDATA[uint32_t	maxlength = inlet_length?inlet_length:attr_table.LENGTHMASK;
uint32_t	startpos = (inlet_startpos<maxlength)?inlet_startpos:maxlength;

uint32_t base = (param_num*(maxlength-startpos))/param_den;

startpos= startpos + base*param_mulstart;
maxlength = startpos + base;

if (startpos>attr_table.LENGTHMASK)
startpos =0;
if (maxlength>attr_table.LENGTHMASK)
maxlength = attr_table.LENGTHMASK;

play = inlet_play;
hold = inlet_hold;]]></code.krate>
         <code.srate><![CDATA[if (play && !hold)
counter++;
else if (!play)
counter = startpos;

if (counter>maxlength)
	counter = startpos;


outlet_pos = counter;]]></code.srate>
      </object>
   </patchobj>
