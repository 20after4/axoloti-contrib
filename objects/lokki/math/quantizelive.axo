<objdefs appVersion="1.0.11">
   <obj.normal id="quantizelive" uuid="63bfc24f-4592-47a1-bd28-36e3476bac2f">
      <sDescription>quantize to n bits, via cc control. every 5 steps the number of bits is increased by one. 0 = 1 bit, 5 =Â 2 bits, 10= 3 bits, etc...</sDescription>
      <author>Simon Iten</author>
      <license>BSD</license>
      <helpPatch></helpPatch>
      <inlets>
         <frac32buffer name="a" description="a"/>
         <int32.positive name="cc" description="use cc i here, every 5 steps the bitrate is increased by one"/>
      </inlets>
      <outlets>
         <frac32buffer name="b" description="quant(a)"/>
      </outlets>
      <displays/>
      <params/>
      <attribs/>
      <code.declaration><![CDATA[int bits;]]></code.declaration>
      <code.krate><![CDATA[bits= (int)(inlet_cc*0.2) + 1;]]></code.krate>
      <code.srate><![CDATA[outlet_b= inlet_a & (~((1<<(28-bits))-1));]]></code.srate>
   </obj.normal>
</objdefs>