   <patchobj type="patch/object" uuid="9da20af5-779f-4669-bc6f-30d43c9ea90e" name="object_1" x="910" y="420">
      <params>
         <bool32.tgl name="use4ppq" value="0"/>
         <frac32.u.map name="decay" value="13.5"/>
         <frac32.u.map name="pitch1" value="7.5"/>
         <frac32.u.map name="pitch2" value="6.5"/>
      </params>
      <attribs/>
      <object id="patch/object" uuid="9da20af5-779f-4669-bc6f-30d43c9ea90e">
         <sDescription>Metronome</sDescription>
         <author>Sputnki</author>
         <license>BSD</license>
         <inlets>
            <int32 name="4ppq"/>
            <int32 name="24ppq"/>
         </inlets>
         <outlets>
            <frac32buffer name="o"/>
         </outlets>
         <displays/>
         <params>
            <bool32.tgl name="use4ppq" description="choose between 24ppq and 4ppq inlet"/>
            <frac32.u.map name="decay"/>
            <frac32.u.map name="pitch1"/>
            <frac32.u.map name="pitch2"/>
         </params>
         <attribs/>
         <includes/>
         <code.declaration><![CDATA[bool ptrig = 0;

int32_t phasor1=0;
int32_t phasor2=0;
int32_t ramp = 0;
int stage = 0 ;]]></code.declaration>
         <code.krate><![CDATA[int factor = 24;
int ppq = inlet_24ppq;

if (param_use4ppq)
{
	factor = 4;
	ppq = inlet_4ppq;
}

int32_t factor4 = factor<<2;

if (factor4*(ppq/(factor4)) == ppq && !ptrig)
{
	ptrig = 1;
	stage = 1;
	ramp = 1<<27;
}
else if (factor*(ppq/factor) == ppq && !ptrig)
{
	ptrig = 1;
	stage = 2;
	ramp = 1<<27;
}
else if (!      ((factor*(ppq/factor))== ppq)      )
	ptrig = 0;


if (stage);
	ramp-=(1<<21)-(param_decay>>6);

if (ramp<0)
{
	ramp = 0;
	stage = 0;
}]]></code.krate>
         <code.srate><![CDATA[phasor1+= param_pitch1+(1<<25);
phasor2+= param_pitch2+(1<<24);

outlet_o = ___SMMUL(ramp,stage==0?0:(stage==1?phasor1:phasor2));]]></code.srate>
      </object>
   </patchobj>
